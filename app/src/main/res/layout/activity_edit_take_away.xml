<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffebcc"
    tools:context=".editDelivary">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ff9800"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:title="   Edit TakeAway" />

    <!--    <ImageView-->
    <!--        android:id="@+id/imageView1"-->
    <!--        android:layout_width="69dp"-->
    <!--        android:layout_height="wrap_content"-->
    <!--        android:layout_alignParentEnd="true"-->
    <!--        android:layout_marginEnd="340dp"-->
    <!--        android:padding="16dp"-->
    <!--        android:src="@drawable/ic_baseline_arrow_back_24"-->
    <!--        tools:layout_editor_absoluteX="354dp"-->
    <!--        tools:layout_editor_absoluteY="0dp" />-->

    <EditText
        android:id="@+id/etName"
        android:layout_width="346dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Kamal" />

    <EditText
        android:id="@+id/edPhone"
        android:layout_width="338dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etName"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:ems="10"
        android:inputType="phone"
        android:text="012345689" />

    <Button
        android:id="@+id/dtebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/edPhone"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="50dp"
        android:text="@string/dtebutton"
        android:textColor="@color/white"
        app:backgroundTint="#ffd633" />

    <Button
        android:id="@+id/tmebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dtebutton"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="38dp"
        android:text="@string/tmebutton"
        android:textColor="@color/white"
        app:backgroundTint="#ffd633" />

    <TextView
        android:id="@+id/edDateTextView"
        android:layout_width="204dp"
        android:layout_height="41dp"
        android:layout_below="@+id/edPhone"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:text="Sep 1, 2021" />

    <TextView
        android:id="@+id/edTimeView"
        android:layout_width="202dp"
        android:layout_height="39dp"
        android:layout_below="@+id/edDateTextView"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="54dp"
        android:layout_marginRight="54dp"
        android:text="10:45 PM" />

    <Button
        android:id="@+id/sveChnge1"
        android:layout_width="166dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="137dp"
        android:layout_marginRight="137dp"
        android:layout_marginBottom="145dp"

        android:text="@string/sveChnge1"
        android:textColor="@color/white"
        app:backgroundTint="#ff9800" />

</RelativeLayout>